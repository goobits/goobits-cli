# Fish completion script for Demo Rust CLI (demo_rust)
# Auto-generated by goobits-cli

# Main completion function
function __demo_rust_complete
    set -l cmd (commandline -opc)
    set -l current (commandline -ct)
    
    # Try dynamic completion first
    if __demo_rust_dynamic_complete $cmd $current
        return 0
    end
    
    # Fallback to static completion
    __demo_rust_static_complete $cmd $current
end

# Dynamic completion using CLI's built-in completion
function __demo_rust_dynamic_complete
    set -l cmd $argv[1..-2]
    set -l current $argv[-1]
    
    # Check if CLI supports completion
    if not command -v demo_rust >/dev/null 2>&1
        return 1
    end
    
    # Get completion suggestions from the CLI itself
    set -l suggestions (demo_rust _completion fish (commandline -cp) (commandline -C) 2>/dev/null)
    
    if test $status -eq 0; and test -n "$suggestions"
        for suggestion in $suggestions
            echo $suggestion
        end
        return 0
    end
    
    return 1
end

# Static completion as fallback
function __demo_rust_static_complete
    set -l cmd $argv[1..-2]
    set -l current $argv[-1]
    
    # Remove the command name itself
    set -e cmd[1]
    
    # If no subcommand yet, complete with main commands
    if test (count $cmd) -eq 0
        __demo_rust_complete_commands
        return 0
    end
    
    # Handle subcommand completion
    switch $cmd[1]
        case 'greet'
            __demo_rust_complete_greet_options $cmd[2..-1]
        case 'info'
            __demo_rust_complete_info_options $cmd[2..-1]
        case 'plugin'
            __demo_rust_complete_plugin $cmd[2..-1]
        case 'upgrade'
            __demo_rust_complete_upgrade $cmd[2..-1]
        case 'help'
            __demo_rust_complete_commands
    end
end

