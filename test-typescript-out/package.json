{
  "name": "test-typescript-cli",
  "version": "1.0.0",
  "description": "Testing TypeScript generation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "testts": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/bin/cli.js",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "dev": "npm run build && node dist/bin/cli.js",
    "completion:install": "node dist/bin/cli.js completion install",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "cli",
    "typescript"  ],
  "author": "Unknown Author",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "reflect-metadata": "^0.2.0"  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.10.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.2.5",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": ""
}