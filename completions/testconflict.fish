# Fish completion script for TestConflictCLI (testconflict)
# Auto-generated by goobits-cli

# Main completion function
function __testconflict_complete
    set -l cmd (commandline -opc)
    set -l current (commandline -ct)
    
    # Try dynamic completion first
    if __testconflict_dynamic_complete $cmd $current
        return 0
    end
    
    # Fallback to static completion
    __testconflict_static_complete $cmd $current
end

# Dynamic completion using CLI's built-in completion
function __testconflict_dynamic_complete
    set -l cmd $argv[1..-2]
    set -l current $argv[-1]
    
    # Check if CLI supports completion
    if not command -v testconflict >/dev/null 2>&1
        return 1
    end
    
    # Get completion suggestions from the CLI itself
    set -l suggestions (testconflict _completion fish (commandline -cp) (commandline -C) 2>/dev/null)
    
    if test $status -eq 0; and test -n "$suggestions"
        for suggestion in $suggestions
            echo $suggestion
        end
        return 0
    end
    
    return 1
end

# Static completion as fallback
function __testconflict_static_complete
    set -l cmd $argv[1..-2]
    set -l current $argv[-1]
    
    # Remove the command name itself
    set -e cmd[1]
    
    # If no subcommand yet, complete with main commands
    if test (count $cmd) -eq 0
        __testconflict_complete_commands
        return 0
    end
    
    # Handle subcommand completion
    switch $cmd[1]
        case 'config'
            __testconflict_complete_config_options $cmd[2..-1]
        case 'completion'
            __testconflict_complete_completion_options $cmd[2..-1]
        case 'daemon'
            __testconflict_complete_daemon_options $cmd[2..-1]
        case 'plugin'
            __testconflict_complete_plugin_options $cmd[2..-1]
        case 'plugin'
            __testconflict_complete_plugin $cmd[2..-1]
        case 'upgrade'
            __testconflict_complete_upgrade $cmd[2..-1]
        case 'help'
            __testconflict_complete_commands
    end
end

